#view current branch or remotes
git branch -v
git remote -v

#delete branch
git branch -d branch-name
git remote rm remote-name

# branches
git branch xxx
git remote add origin_xxx remote_url
git checkout branch_name

#replace old origin url
git remote set-url origin new_git_url

#git version
# check what file in cache
git ls-file

#delete the last commit
git reset HEAD~1

#add all files
git add .

#add certain file
git add xxx_file

#commit locally
git commit -m "msg put here"

# push to remote url-origin
git push origin branch_name


